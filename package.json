{
  "name": "mpeztrack",
  "version": "1.0.0",
  "description": "a codeless implementation of mixpanel-browser",
  "main": "./src/index.js",
  "scripts": {
    "test": "test",
    "watch": "npm-watch build",
    "server": "./node_modules/.bin/serve ./dist",
    "build": "npm run min && npm run max",
    "min": "./node_modules/.bin/esbuild ./src/index.js --bundle --minify --tree-shaking=true --outfile=./dist/eztrack.min.js",
    "max": "./node_modules/.bin/esbuild ./src/index.js --bundle --outfile=./dist/eztrack.js"
  },
  "watch": {
    "build": "./src/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ak--47/mpEZTrack.git"
  },
  "author": "AK",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ak--47/mpEZTrack/issues"
  },
  "homepage": "https://github.com/ak--47/mpEZTrack#readme",
  "devDependencies": {
    "esbuild": "^0.15.7",
    "eslint": "^8.23.0",
    "esprima": "^4.0.1",
    "jest": "^29.0.3",
    "nodemon": "^2.0.19",
    "npm-watch": "^0.11.0",
    "serve": "^14.0.1"
  },
  "dependencies": {
    "mixpanel-browser": "^2.45.0",
    "query-selector-shadow-dom": "^1.0.0"
  }
}
